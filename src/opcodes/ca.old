package opcodes

import "emulator/src/cpu"

// CA 2nnn - Call subroutine at nnn.
func CA(c *cpu.CPU, opcode uint16) {
	c.Sp++
	c.Stack[c.Sp] = c.Pc

	// recuperer juste l'address qui est defini par NNN
	c.Pc = (opcode & 0x0FFF)

	// moins 2 parce que plus 2 va etre appliquer a la fin du switch
	c.Pc -= 2
}
